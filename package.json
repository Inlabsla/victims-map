{
  "name": "api-template",
  "version": "0.1.0",
  "description": "Plantilla para la construccion de microservicios de backend (REST API) en MBaaS Davivienda",
  "repository": {
    "type": "git",
    "url": "https://source.cloud.google.com/mbaas-desarrollo/api-template"
  },
  "keywords": [
    "API"
  ],
  "author": "DXC - Davivienda",
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "tsc": "tsc",
    "build": "npm-run-all clean tsc",
    "dev:start": "npm-run-all build start",
    "dev": "nodemon --watch src -e ts,ejs,json --exec npm run dev:start",
    "start": "node .",
    "test": "jest",
    "lint-report": "node tslintReport.js"
  },
  "_moduleAliases": {
    "~": "dist",
    "@config": "dist/config",
    "@models": "dist/models",
    "@routes": "dist/routes",
    "@services": "dist/services",
    "@utils": "dist/utils"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "helmet": "^3.15.0",
    "inversify": "^5.0.1",
    "inversify-binding-decorators": "^4.0.0",
    "inversify-express-utils": "^6.3.2",
    "joi": "^14.3.1",
    "lightship": "^3.0.0",
    "lodash": "^4.17.21",
    "log4js": "^3.0.6",
    "module-alias": "^2.1.0",
    "morgan": "^1.9.1",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^7.0.0-alpha.4",
    "sequelize-typescript": "^2.1.3",
    "swagger-ui-express": "^4.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@types/body-parser": "^1.17.0",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.0",
    "@types/helmet": "0.0.42",
    "@types/inversify-express-utils": "^2.0.0",
    "@types/jest": "^23.3.12",
    "@types/joi": "^14.0.1",
    "@types/lodash": "^4.14.181",
    "@types/log4js": "^2.3.5",
    "@types/module-alias": "^2.0.0",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.17.60",
    "@types/supertest": "^2.0.7",
    "@types/swagger-ui-express": "^3.0.0",
    "@types/validator": "^13.7.2",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "node-mocks-http": "^1.7.3",
    "nodemon": "^1.18.9",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "rimraf": "^2.6.2",
    "supertest": "^3.3.0",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-config-security": "^1.14.0",
    "tslint-html-report": "^2.0.2",
    "typescript": "^4.1.3"
  }
}
